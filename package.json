{
  "name": "SaveDataSynchronizer",
  "version": "1.0.1",
  "description": "An Electron application with React and TypeScript",
  "repository": "https://github.com/electron/fiddle",
  "main": "./out/main/index.js",
  "author": {
    "name": "Emiliopg91",
    "email": "ojosdeserbio@gmail.com",
    "url": "https://electron-react-boilerplate.js.org"
  },
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "concurrently 'pnpm:typecheck:node' 'pnpm:typecheck:web'",
    "start": "electron-vite preview",
    "dev": " electron-vite dev --watch",
    "build": "pnpm lint && pnpm format && pnpm typecheck && electron-vite build",
    "update": "pnpm store prune && pnpm i",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm build && electron-builder --dir",
    "build:win": "pnpm build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@tser-framework/commons": "^2.0.34",
    "@tser-framework/main": "^2.0.136",
    "@tser-framework/renderer": "^2.0.60",
    "async-mutex": "^0.5.0",
    "bootstrap": "^5.3.3",
    "electron-log": "^5.1.5",
    "electron-prompt": "^1.7.0",
    "electron-updater": "^6.2.1",
    "primereact": "^10.6.6",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "systeminformation": "^5.22.10"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "prettier": "3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-dynamic-import": "^1.5.0"
  },
  "extraResources": [
    "./assets/**"
  ]
}
